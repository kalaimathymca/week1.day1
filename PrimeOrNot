package week1.day1;

public class Primenumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

int n=2;
int m=0;
for(int i= 2; i<= n-1; i++) {
	if(n%i== 0) {
		m= 1;
	
	     break;
		
	}
}

if(m== 1) {
	System.out.println("The number "+n+" is not a prime");
}else{
	System.out.println("The number "+n+" is a prime");
}
	
 
	
	}

}
